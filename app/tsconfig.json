{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
,
"app/(tabs)/_layout.jsx",
"app/+not-found.jsx",
"app/_layout.jsx",
"app/(tabs)/notes.jsx",
"app/(tabs)/index.jsx"
,
"components/ThemedView.jsx",
"components/ThemedText.jsx",
"components/ParallaxScrollView.jsx",
"components/HelloWave.jsx",
"components/HapticTab.jsx",
"components/ExternalLink.jsx",
"components/Collapsible.jsx"
]
}
